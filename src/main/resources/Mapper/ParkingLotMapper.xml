<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tws.repository.ParkingLotMapper">
    <select id="selectAll" resultType="tws.entity.ParkingLot">
        select * from `parkingLot`;
    </select>

    <insert id="insert">
    insert  into `parkingLot` values(#{parkingLot.parkingLotID},#{parkingLot.capacity},#{parkingLot.availablePositionCount});
    </insert>

</mapper>